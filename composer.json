{
    "name": "code16/responder",
    "description": "Generic API response handler for Laravel framework",
    "license": "MIT",
    "authors": [
        {
            "name": "RÃ©mi Collin",
            "email": "remi@code16.fr"
        }
    ],
    "require": {
        "illuminate/support": "5.6.*|5.7.*",
        "illuminate/http": "5.6.*|5.7.*",
        "illuminate/contracts": "5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.0.*",
        "orchestra/testbench": "3.6.*|3.7.*",
        "fzaninotto/faker": "~1.4",
        "ramsey/uuid": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Code16\\Responder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code16\\Responder\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
          "providers": [
              "Code16\\Responder\\ResponderServiceProvider"
          ],
          "aliases": {
            "Responder": "Code16\\Responder\\Responder"
          }
      }
    }
}
